a = int(input('Количество билетов='))        #Ввод количества билетов
c=[]                                         #Пустой список

for i in range(a):                           #Цикл (for) для переменной (b) в количестве (а) раз
   b = int(input('Возраст посетителей='))    #Ввод восроста посетителей, через (Enter)
   if b < 18:                                #Условие переменная (b) меньше (18)
        c.append(0)                          #Добавление числа (0) в список (с)
   elif 18 <= b <= 25:                       #Условие переменная (b) больше или равна (18) и (b) меньше или равна (25)
        c.append(990)                        #Добавление числа (990) в список (с)
   elif b > 25:                              #Условие переменная (b) больше (25)
        c.append(1390)                       #Добавление числа (1390) в список (с)

print(c)                                     #Проверка, вывод списка (с)
print(sum(c))                                #Проверка, вывод суммы элементов списка (с)
c=float(sum(c))                              #Присваевание сумме списка(с) тип (float)-число с плавующей точкой

if a<3:                                      #Условие переменная (а) меньше (3)
    c=c                                      #Переменная (с) не изменяется
else:                                        #Условие (ИНАЧЕ)
    c=c*0.9                                  #Учет скидки в 10%

print('Выбранно билетов=',a)                 #Вывод количества выбранных билетов
print('Общая стоимость билетов=',c)          #Вывод общей суммы за билеты
